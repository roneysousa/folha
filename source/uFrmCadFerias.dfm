  object dstRelFolha: TSQLDataSet
    CommandText = 
      'Select FO.FUN_MATRICULA, FU.FUN_NOME, FU.FUN_ADMISSAO, FU.FUN_EM' +
      'PRESA, FO.FOL_DTGERACAO, FU.FUN_CPF, '#13#10' FO.FOL_BENEFICIO, FO.FOL' +
      '_QTBENEFICIO, FO.FOL_PEBENEFICIO, FO.FOL_NMBENEFICIO, '#13#10' FO.FOL_' +
      'DESBEN, FO.FOL_VLBENEFICIO, C.CBO_DESCRICAO '#13#10' from FUNCIONARIOS' +
      ' FU INNER join FOLHA FO ON FU.FUN_MATRICULA = FO.FUN_MATRICULA '#13 +
      #10' INNER join CBO C ON C.CBO_CODIGO = FU.FUN_CBO '#13#10' Where (FO.FUN' +
      '_MATRICULA = :pMATRICULA) '#13#10' AND (FU.FUN_DTINICIO_FERIAS = :pGOZ' +
      'O) '#13#10' Group by FO.FUN_MATRICULA, FU.FUN_NOME, FU.FUN_ADMISSAO, F' +
      'U.FUN_EMPRESA, FO.FOL_DTGERACAO, FU.FUN_CPF,'#13#10' FO.FOL_BENEFICIO,' +
      ' FO.FOL_QTBENEFICIO, FO.FOL_PEBENEFICIO, FO.FOL_NMBENEFICIO, '#13#10' ' +
      'FO.FOL_DESBEN, FO.FOL_VLBENEFICIO, C.CBO_DESCRICAO'#13#10' order by FO' +
      '.FUN_MATRICULA, FO.FOL_BENEFICIO'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pMATRICULA'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'pGOZO'
        ParamType = ptInput
      end>
    SQLConnection = dmFolha.sqlCon_Folha
