  object dstEspelho: TSQLDataSet
    CommandText = 
      'select P.PNT_FUNCIONARIO, F.FUN_NOME, C.CBO_DESCRICAO, P.PNT_DAT' +
      'AENTRADA, P.PNT_DATASAIDA, P.PNT_FLPRESENCA,'#13#10'P.PNT_HORAENTRADA,' +
      ' P.PNT_HORASAIDA from PONTO P'#13#10'INNER join FUNCIONARIOS F ON F.FU' +
      'N_MATRICULA = P.PNT_FUNCIONARIO'#13#10'INNER join CBO C ON F.FUN_CBO =' +
      ' C.CBO_CODIGO'#13#10'Where (P.PNT_FUNCIONARIO = :pFUNCIONARIO)'#13#10'and (P' +
      '.PNT_DATAENTRADA >= :pDTINICIAL) '#13#10'and (P.PNT_DATAENTRADA <= :pD' +
      'TFINAL)'#13#10'and ((P.PNT_FLPRESENCA = :pFLSITU1) OR (P.PNT_FLPRESENC' +
      'A = :pFLSITU2))'#13#10'order by P.PNT_DATAENTRADA, P.PNT_HORAENTRADA'#13#10
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pFUNCIONARIO'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftDate
        Name = 'pDTINICIAL'
        ParamType = ptInput
        Value = 0d
      end
      item
        DataType = ftDate
        Name = 'pDTFINAL'
        ParamType = ptInput
        Value = 0d
      end
      item
        DataType = ftString
        Name = 'pFLSITU1'
        ParamType = ptInput
        Value = ''
      end
      item
        DataType = ftString
        Name = 'pFLSITU2'
        ParamType = ptInput
        Value = ''
      end>
    SQLConnection = dmFolha.sqlCon_Folha
    Left = 144
    Top = 136
    object dstEspelhoPNT_FUNCIONARIO: TIntegerField
      FieldName = 'PNT_FUNCIONARIO'
