  object dstEspelho: TSQLDataSet
    CommandText = 
      'select P.PNT_FUNCIONARIO, F.FUN_NOME, C.CBO_DESCRICAO, P.PNT_DAT' +
      'AENTRADA, P.PNT_DATASAIDA, P.PNT_FLPRESENCA,'#13#10'P.PNT_HORAENTRADA,' +
      ' P.PNT_HORASAIDA from PONTO P'#13#10'INNER join FUNCIONARIOS F ON F.FU' +
