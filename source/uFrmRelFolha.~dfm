  object dstRelFolha: TSQLDataSet
    CommandText = 
      'Select F.FOL_CODIGO, F.FUN_MATRICULA, FU.FUN_NOME, FU.FUN_ADMISS' +
      'AO, F.FOL_DTGERACAO, FU.FUN_CPF,'#13#10'F.FOL_BENEFICIO, F.FOL_QTBENEF' +
      'ICIO, F.FOL_PEBENEFICIO, F.FOL_NMBENEFICIO, F.FOL_DESBEN,'#13#10'F.fol' +
      '_vlbeneficio, C.CBO_DESCRICAO'#13#10'from FOLHA F'#13#10'INNER join FUNCIONA' +
      'RIOS FU ON FU.FUN_MATRICULA = F.FUN_MATRICULA'#13#10'INNER join CBO C ' +
      'ON C.CBO_CODIGO = FU.FUN_CBO'#13#10'order by F.FUN_MATRICULA, F.FOL_BE' +
      'NEFICIO;'
    MaxBlobSize = -1
    Params = <>
    SQLConnection = dmFolha.sqlCon_Folha
    Left = 248
    Top = 37
    object dstRelFolhaFOL_CODIGO: TStringField
